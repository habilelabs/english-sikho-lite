<%- contentFor('body') %>
<div class="headerSection">
    <div class="container">
        <div class="Word"><a href="/"> Word</a></div>
        <div class="startingwithletter">
            <h1>List of words starting with letter <%= param.word %></h1>
            <ul>
                <%
                for(var k = 0; k < param.data.length;k++)
                { %>
                <li><a href="/word/<%= param.data[k].word %>">
                        <%= param.data[k].word %>
                    </a></li>
                <%
                }
                %>
            </ul>
        </div>
        
        <div class="Pagination">
            <ul class="pagination">
  <li><a href="#">«</a></li>
  <li><a href="#">1</a></li>
  <li><a class="active" href="#">2</a></li>
  <li><a href="#">3</a></li>
  <li><a href="#">4</a></li>
  <li><a href="#">5</a></li>
  <li><a href="#">6</a></li>
  <li><a href="#">7</a></li>
  <li><a href="#">»</a></li>
</ul>

        </div>

    </div>
</div>