<%- contentFor('body') %>
  <div class="headerSection">
          <div class="container">

          <div class="Word"><a href="/"> Home</a></div>
          <div class="logo">
                 <img src="/logo.png">
              </div>
          <div class="searchBar">
                <input id="searchWord" type="text" placeholder="Type a word or a phrase to search">
                <button class="btn search-submit" type="button" onclick="search()"><span>Search</span></button>           
                </div>

              <div class="searchBar center">

              <h1><%=param.data.word%></h1> 

              <p><%=param.data.type%></p> 

              <p><%=param.data.hindiSentence%></p>

              <h4>Definitions :</h4>

              <p><%=param.data.definition%></p>

              <h4>Synonym :</h4>

              <p><%=param.data.synonym%></p>
              
              </div>
          </div>
  </div>