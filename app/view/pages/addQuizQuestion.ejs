<%- contentFor('body') %><div class="container"><div class="row"><div class="col-md-9"><div class="panel panel-primary wotd"><div class="panel-heading"><i class="fa fa-plus-square circle-blue"></i> <span id="quizPageTitle">Add Quiz Question</span></div><input type="hidden" id="questionId"><div class="panel-body"><div class="container-fluid"><div class="success-msg hideMe" id="addQuizQuestionSuccessMsgID"></div><div class="errorMsg hideMe" id="addQuizQuestionErrorMsgID"></div><form id="add-quiz-question-form" class="form-horizontal marginTop10" method="post"><div class="form-group"><input type="text" class="form-control" placeholder="Topic" name="title" id="questionTitle" required></div><div class="form-group"><select class="form-control" id="quizQuestionCategory" required><option value="" disabled=disabled>Select Quiz Category</option></select></div><div class="form-group"><select class="form-control" id="quizQuestionType" onchange="selectedQuestionType()" required><option value="" disabled=disabled>Select Answer Type</option></select></div><div class="form-group"><textarea class="form-control" id="input"></textarea><span id="blogError"></span></div><div class="form-group" id="answerDiv"><label>Answer :</label><div id="questionTypeTf"><div class="row"><div class="col-md-2"><input type="radio" value="true" name="tf" id="tfTrue"> True</div></div><div class="row"><div class="col-md-2"><input type="radio" value="false" name="tf" id="tfFalse"> False</div></div></div><div id="questionType4option"><div class="row marginBottom"><div class="col-md-1"><input type="radio" value="1" name="4option" id="radioOptionInput1"></div><div class="col-md-6"><input type="text" class="form-control" placeholder="Option 1" id="4optionInput1"></div></div><div class="row marginBottom"><div class="col-md-1"><input type="radio" value="2" name="4option" id="radioOptionInput2"></div><div class="col-md-6"><input type="text" class="form-control" placeholder="Option 2" id="4optionInput2"></div></div><div class="row marginBottom"><div class="col-md-1"><input type="radio" value="3" name="4option" id="radioOptionInput3"></div><div class="col-md-6"><input type="text" class="form-control" placeholder="Option 3" id="4optionInput3"></div></div><div class="row marginBottom"><div class="col-md-1"><input type="radio" value="4" name="4option" id="radioOptionInput4"></div><div class="col-md-6"><input type="text" class="form-control" placeholder="Option 4" id="4optionInput4"></div></div></div><div id="questionTypeTyping"><input type="text" class="form-control" placeholder="Answer here" name="questionTypeTypingTxt" id="questionTypeTypingAnswer"></div></div><div class="form-group"><input type="text" class="form-control" placeholder="Display a message after correct answer" name="correct_msg" id="correct_msg" required></div><div class="form-group"><input type="text" class="form-control" placeholder="Display a message after wrong answer" name="incorrect_msg" id="incorrect_msg" required></div><div class="form-group"><input type="text" class="form-control" placeholder="Hint for question" name="hint" id="hint" required></div><div class="form-group"><div class="row"><div class="col-md-6"><input type="checkbox" placeholder="Show point in box" name="show_points_in_box" id="show_points_in_box"> Show Points In Box</div><div class="col-md-6"><input type="checkbox" placeholder="Disable to view Correct Answer" name="disable_correct" id="disable_correct"> Disable to view Correct Answer</div></div></div><div class="form-group"><div class="row"><div class="col-md-4"><input type="number" class="form-control" placeholder="Max Points" name="maxPoints" id="maxPoints" required></div><div class="col-md-4"><input type="number" class="form-control" placeholder="Time (Seconds)" name="time" id="time" required></div><div class="col-md-4"><input type="number" class="form-control" placeholder="Order No (optional)" name="orderNo" id="orderNo"></div></div></div><div class="form-group special-buttons text-center"><button id="addBlog-button" type="submit" class="btn btn-lg btn-login">Submit !</button></div></form></div></div><div class="panel-heading bottom"></div></div></div><div class="col-md-3 sidebar"><div class="panel panel-primary wog"><div class="panel-heading"><i class="fa fa-trophy circle-red"></i> WORD GAMES</div><div class="panel-body"><h4 class="setSize"><a class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-book circle-red"></i> Vocabulary Test</a></h4><h4 class="setSize"><a onclick="getQuizTitle(&quot;Grammar&quot;)" class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-font circle-red"></i> Grammar Test</a></h4><h4 class="setSize"><a class="spellChallenge" data-backdrop="static" data-toggle="modal" data-target="#spellChallenge" href="#"><i class="fa fa-pencil circle-red"></i> Spell Challenge</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#nameThing" href="#"><i class="fa fa-picture-o circle-red"></i> 4 Pics 1 Word</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#leaderBoardModal" href="#"><i class="fa fa-trophy circle-red"></i> Leader Boards</a></h4></div></div><div class="panel panel-primary side-posts blogPanel"><ul class="nav nav-tabs" role="tablist"><li role="presentation" class="active"><a href="#category" aria-controls="top" role="tab" data-toggle="tab">Category</a></li><li role="presentation"><a href="#recent" aria-controls="recent" role="tab" data-toggle="tab">Recent</a></li><li role="presentation"><a href="#top" aria-controls="top" role="tab" data-toggle="tab">Popular</a></li></ul><div class="panel-body sideBarPanelBody"><div class="tab-content"><div role="tabpanel" class="tab-pane active" id="category"><% for (var i = 0; i < param.categoryList.length; i++) { %><div class="media"><div class="row"><div class='col-xs-12'><div class="media-body"><a href="/blog/category/<%= param.categoryList[i].replace(/ /g, '-') %>"><h5 class="media-heading"><%= param.categoryList[i] %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="recent"><% for (var n = 0; n < param.blogList.length; n++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><img class="media-object" src="<%= param.blogList[n].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.blogList[n].title %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="top"><% for (var k = 0; k < param.topBlog.length; k++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><img class="media-object" src="<%= param.topBlog[k].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.topBlog[k].title %></h5></a></div></div></div></div><% } %></div></div></div></div></div></div></div>