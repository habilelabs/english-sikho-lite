<%- contentFor('body') %><div class="container"><div class="row"><div class="col-md-9"><div class="word-content"><div class="row"><% if(individualBlog != null) {%><div class="col-md-3"><div class="blog_image"><img class="img-responsive" onerror="this.onerror=null,this.src=&quot;/img/no-image.jpg&quot;" src="<%= individualBlog.url %>" alt="Born"></div><div class="setFacebook"><div class="fb-like" data-href="https://www.englishsikho.com/<%= individualBlog.urlFriendlyTitle %>" data-layout="button_count" data-action="like" data-show-faces="true"></div></div><ul class="social-networks list-inline"><li class="facebook"><a class="fa fa-facebook" target="_blank" href="https://www.facebook.com/sharer/sharer.php?u=englishsikho.com/blog/<%= individualBlog.urlFriendlyTitle %>" data-toggle="tooltip" data-placement="bottom" title="Share on Facebook"></a></li><li class="googleplus"><a class="fa fa-google-plus" href="https://plus.google.com/share?url=englishsikho.com/blog/<%= individualBlog.urlFriendlyTitle %>" target="_blank" data-toggle="tooltip" data-placement="bottom" title="Share on Google+"></a></li><li class="twitter"><a class="fa fa-twitter" href="https://twitter.com/share?text=<%= individualBlog.urlFriendlyTitle %> via @englishsikho&url=https://www.englishsikho.com/blog/<%= individualBlog.urlFriendlyTitle %>" target="_blank" data-toggle="tooltip" data-placement="bottom" title="Share on Twitter"></a></li></ul></div><div class="col-md-9"><div class="row"><div class="col-md-12"><% if(individualBlog.title){ %><p class="word-eng leftSet"><%= individualBlog.title %></p><% }%></div></div><% if(individualBlog.content){ %><hr class="dash-hr"><p><%- individualBlog.content %></p><% } %><% if(!param.preview) { %><div class="row"><% if(previousBlog!=null) { %><div class="col-xs-6 col-sm-6 col-md-6"><a href="/blog/<%= previousBlog.urlFriendlyTitle %>" class="btn btn-primary"><i class="fa fa-angle-double-left"></i> Previous</a></div><% } if(nextBlog != null) { %><div class="col-xs-6 col-sm-6 col-md-6"><a href="/blog/<%= nextBlog.urlFriendlyTitle %>" class="btn btn-primary pull-right">Next <i class="fa fa-angle-double-right"></i></a></div><% } %></div><% } %><hr class="dash-hr"><% if(param.user){ if(param.userId == individualBlog.reference ) { %><a class="authorEdit btn btn-primary" href='/blog/edit/<%= individualBlog.urlFriendlyTitle %>'><i class="fa fa-pencil-square-o"></i></a><% }} %> <span class="authorName"><h4><%= individualBlog.author %></h4></span><br><br><hr class="dash-hr"><div id="fb-root"></div><div class="rows"><div class="col-md-12"><div class="fb-comments" data-href="https://www.englishsikho.com/blog/<%= individualBlog.urlFriendlyTitle %>" data-numposts="5"></div></div></div></div><% } else {%><div class="col-md-12"><p class="word-eng text-center">Blog is not available!!</p></div><% } %></div></div></div><div class="col-md-3 sidebar"><div class="panel panel-primary wog"><div class="panel-heading"><i class="fa fa-trophy circle-red"></i> WORD GAMES</div><div class="panel-body"><h4 class="setSize"><a class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-book circle-red"></i> Vocabulary Test</a></h4><h4 class="setSize"><a onclick="getQuizTitle(&quot;Grammar&quot;)" class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-font circle-red"></i> Grammar Test</a></h4><h4 class="setSize"><a class="spellChallenge" data-backdrop="static" data-toggle="modal" data-target="#spellChallenge" href="#"><i class="fa fa-pencil circle-red"></i> Spell Challenge</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#nameThing" href="#"><i class="fa fa-picture-o circle-red"></i> 4 Pics 1 Word</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#leaderBoardModal" href="#"><i class="fa fa-trophy circle-red"></i> Leader Boards</a></h4></div></div><div class="panel panel-primary side-posts blogPanel"><ul class="nav nav-tabs" role="tablist"><li role="presentation" class="active"><a href="#category" aria-controls="top" role="tab" data-toggle="tab">Category</a></li><li role="presentation"><a href="#recent" aria-controls="recent" role="tab" data-toggle="tab">Recent</a></li><li role="presentation"><a href="#top" aria-controls="top" role="tab" data-toggle="tab">Popular</a></li></ul><div class="panel-body sideBarPanelBody"><div class="tab-content"><div role="tabpanel" class="tab-pane active" id="category"><% for (var i = 0; i < param.categoryList.length; i++) { %><div class="media"><div class="row"><div class='col-xs-12'><div class="media-body"><a href="/blog/category/<%= param.categoryList[i].replace(/ /g, '-') %>"><h5 class="media-heading"><%= param.categoryList[i] %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="recent"><% for (var n = 0; n < param.blogList.length; n++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><img class="media-object" src="<%= param.blogList[n].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.blogList[n].title %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="top"><% for (var k = 0; k < param.topBlog.length; k++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><img class="media-object" src="<%= param.topBlog[k].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.topBlog[k].title %></h5></a></div></div></div></div><% } %></div></div></div></div></div></div></div>