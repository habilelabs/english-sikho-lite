<%- contentFor('body') %><div class="headerSection"><div class="container"><div class="Word"><a href="browse">Browse by words</a></div><div class="logo"><img src="/logo.svg"></div><div class="searchBar"><input id="searchWord" type="text" placeholder="Type a word or a phrase to search"> <button class="btn search-submit" type="button" onclick="search()"><span>Search</span></button><p class="TopSearch">Top Search..</p></div><div class="TopSearchTitle"><ul><% for(var k = 0; k < param.data.length;k++) { if(param.data[k].word !== 'No results found, Similar word suggestions') { %><li><a href="/word/<%= param.data[k].word %>"><%= k+1 %>.<%= param.data[k].word %></a></li><% } } %></ul></div></div></div>