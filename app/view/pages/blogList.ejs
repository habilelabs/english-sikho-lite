<%- contentFor('body') %><div class="container"><div class="row"><div class="col-md-9"><div class="panel panel-primary wotd"><div class="panel-heading"><i class="fa fa-list circle-blue"></i> List of Blogs</div><div class="panel-body"><% if(blogList ==''){ %><div class="col-xs-12"><br><br><br><h4 class="success-msg">No blogs available</h4><br><br><br></div><% } else{ for(var k =0; k< blogList.length;k++){%><div class='row'><div class="col-md-3 col-xs-3"><a href="/blog/<%= blogList[k].urlFriendlyTitle %>"><img class="img-responsive pointer" onerror="this.onerror=null,this.src=&quot;/img/no-image.jpg&quot;" src="<%= blogList[k].url %>" alt="Born"></a></div><div class="col-md-9 col-xs-9"><div class="row"><div class="col-md-12"><% if(blogList[k].title){ %><p class="word-eng leftSet"><a href="/blog/<%= blogList[k].urlFriendlyTitle %>"><%= blogList[k].title %></a></p><% }%></div></div><% if(blogList[k].content){ %><hr class="dash-hr"><p><%- blogList[k].content.replace(/<td/ig,' <td').replace(/(<([^>]+)>)/ig,'').substring(0, 500) %>......</p><br><% }%><div class="row"><div class="col-md-6"><% if(param.user){ if(param.userId == blogList[k].reference ) { %><a class="btn btn-primary" href="/blog/edit/<%= blogList[k].urlFriendlyTitle %>">Edit</a><% }} %><% if(param.preview) {%> <a class="btn btn-primary" href='/blog/preview/<%= blogList[k].urlFriendlyTitle %>'>Read More</a><%} else {%> <a class="btn btn-primary" href='/blog/<%= blogList[k].urlFriendlyTitle %>'>Read More</a><% } %></div><div class="col-md-6"><% if(param.user && !param.categoryStatus){ if(param.userId == blogList[k].reference ) { if(param.page==1 && k==0){ }else{%><a class="btn btn-primary" onclick="moveUp(&quot;<%= blogList[k]._id %>&quot;)" title="1 position up"><i class="fa fa-chevron-up"></i></a><%} if(param.page==param.lastpage && k==blogList.length-1){ } else{ %> <a class="btn btn-primary" onclick="moveDown(&quot;<%= blogList[k]._id %>&quot;)" title="1 position down"><i class="fa fa-chevron-down"></i></a><%} if(param.page==1 && k==0){ %><%}else{%> <a class="btn btn-primary" onclick="moveFirst(&quot;<%= blogList[k]._id %>&quot;)" title="Move on first position"><i class="fa fa-angle-double-up"></i></a><%} if(param.page==param.lastpage && k==blogList.length-1){ } else{ %> <a class="btn btn-primary" onclick="moveLast(&quot;<%= blogList[k]._id %>&quot;)" title="Move on last position"><i class="fa fa-angle-double-down"></i></a><% }}} %></div></div><hr class="dash-hr"></div></div><% }}%></div><div class="panel-heading bottom"><ul class="list-inline pagination"><% if(param.preview) {%><li><a href="/blog/preview"><i class="fa fa-step-backward"></i></a></li><%} else { if(param.categoryStatus) {%><li><a href="/blog/category/<%= param.categoryName%>"><i class="fa fa-step-backward"></i></a></li><%} else{%><li><a href="/blog/"><i class="fa fa-step-backward"></i></a></li><% }} var startPage=1; var endPage=10; if(param.page > 5){ startPage=parseInt(param.page)-4; endPage=parseInt(param.page)+5; } if(endPage > param.lastpage){ endPage = param.lastpage; } for(var i =startPage; i<=endPage;i++){ if(i==param.page){ %><li class="active"><% } else{ %></li><li><% } %><% if(param.preview) {%><a href="/blog/preview/page/<%= i%>"><%= i%></a><%} else { if(param.categoryStatus){ %> <a href="/blog/category/<%= param.categoryName%>/page/<%= i%>"><%= i%></a><%} else{ %> <a href="/blog/page/<%= i%>"><%= i%></a><%} }%></li><% } %><li><% if(param.preview) {%><a href="/blog/preview/page/<%= param.lastpage%>"><i class="fa fa-step-forward"></i></a><%} else { if(param.categoryStatus){ %> <a href="/blog/category/<%= param.categoryName%>/page/<%= param.lastpage%>"><i class="fa fa-step-forward"></i></a><% } else {%> <a href="/blog/page/<%= param.lastpage%>"><i class="fa fa-step-forward"></i></a><%}}%></li></ul></div></div></div><div class="col-md-3"><div class="panel panel-primary wog"><div class="panel-heading"><i class="fa fa-trophy circle-red"></i> WORD GAMES</div><div class="panel-body"><h4 class="setSize"><a class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-book circle-red"></i> Vocabulary Test</a></h4><h4 class="setSize"><a onclick="getQuizTitle(&quot;Grammar&quot;)" class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-font circle-red"></i> Grammar Test</a></h4><h4 class="setSize"><a class="spellChallenge" data-backdrop="static" data-toggle="modal" data-target="#spellChallenge" href="#"><i class="fa fa-pencil circle-red"></i> Spell Challenge</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#nameThing" href="#"><i class="fa fa-picture-o circle-red"></i> 4 Pics 1 Word</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#leaderBoardModal" href="#"><i class="fa fa-trophy circle-red"></i> Leader Boards</a></h4></div></div><div class="panel panel-primary side-posts blogPanel"><ul class="nav nav-tabs" role="tablist"><li role="presentation" class="active"><a href="#category" aria-controls="top" role="tab" data-toggle="tab">Category</a></li><li role="presentation"><a href="#recent" aria-controls="recent" role="tab" data-toggle="tab">Recent</a></li><li role="presentation"><a href="#top" aria-controls="top" role="tab" data-toggle="tab">Popular</a></li></ul><div class="panel-body sideBarPanelBody"><div class="tab-content"><div role="tabpanel" class="tab-pane active" id="category"><% for (var i = 0; i < param.categoryList.length; i++) { %><div class="media"><div class="row"><div class='col-xs-12'><div class="media-body"><a href="/blog/category/<%= param.categoryList[i].replace(/ /g, '-') %>"><h5 class="media-heading"><%= param.categoryList[i] %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="recent"><% for (var n = 0; n < param.blogList.length; n++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><img class="media-object" src="<%= param.blogList[n].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.blogList[n].title %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="top"><% for (var k = 0; k < param.topBlog.length; k++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><img class="media-object" src="<%= param.topBlog[k].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.topBlog[k].title %></h5></a></div></div></div></div><% } %></div></div></div></div></div></div></div>