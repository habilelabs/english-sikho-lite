<%- contentFor('body') %><div class="container"><div class="row"><div class="col-md-9"><div class="panel panel-primary wotd"><div class="panel-heading"><span class="text-center"><i class="fa fa-list circle-blue"></i> Questions For You</span><p class="hidden" id="askMeUniqueId"><%= param.userId %></p><button id="askQuestionBtn" onclick="viewAskQuestion()" type="button" class="btn btn-primary float-right"><i class="fa fa-question-circle icon-white"></i> Ask Question</button></div><div class="panel-body"><% if(questionList ==''){ %><div class="col-xs-12"><br><br><br><h4 class="success-msg">No questions available</h4><br><br><br></div><% } else{ for(var k =0; k< questionList.length;k++){%><div class='row'><div class="col-md-3 col-xs-3"><a href="/question-page/<%= questionList[k].urlFriendlyTitle %>"><img class="img-responsive pointer" onerror="this.onerror=null,this.src=&quot;/img/no-image.jpg&quot;" src="<%= questionList[k].categoryId.imageThumb %>" alt="Born"></a></div><div class="col-md-8 col-xs-8"><div class="row"><div class="col-md-12"><% if(questionList[k].title){ %><p class="word-eng leftSet"><a href="/question-page/<%= questionList[k].urlFriendlyTitle %>"><%= questionList[k].title %></a></p><% }%></div></div><% if(questionList[k].description){ %><hr class="dash-hr"><p><%- questionList[k].description.replace(/(<([^>]+)>)/ig,'').substring(0, 500) %>......</p><br><% } %><a class="btn btn-primary" href='/question-page/<%= questionList[k].urlFriendlyTitle %>'>Write Answer</a><hr class="dash-hr"></div><div class="col-md-1 col-xs-1"><span onclick="reportQuestion(&quot;<%= questionList[k]._id %>&quot;)" data-toggle="tooltip" data-placement="top" title="" data-original-title="Report this Question" aria-describedby="tooltip649532"><i id="qu<%= questionList[k]._id %>" class="fa fa-bug reportBackground circle-blue" style="background-color:#0e8079"></i></span></div></div><% }}%></div><div class="panel-heading bottom"><ul class="list-inline pagination"><li><a href="/question-answer/"><i class="fa fa-step-backward"></i></a></li><% var startPage=1; var endPage=10; if(param.page > 5){ startPage=parseInt(param.page)-4; endPage=parseInt(param.page)+5; } if(endPage > param.lastpage){ endPage = param.lastpage; } for(var i =startPage; i<=endPage;i++){ if(i==param.page){ %><li class="active"><% } else{ %></li><li><% } %><% if(param.preview) {%><a href="/question-answer/preview/page/<%= i%>"><%= i%></a><%} else {%> <a href="/question-answer/page/<%= i%>"><%= i%></a><%}%></li><% } %><li><% if(param.preview) {%><a href="/question-answer/preview/page/<%= param.lastpage%>"><i class="fa fa-step-forward"></i></a><%} else {%> <a href="/question-answer/page/<%= param.lastpage%>"><i class="fa fa-step-forward"></i></a><%}%></li></ul></div></div></div><div class="col-md-3"><div class="panel panel-primary wog"><div class="panel-heading"><i class="fa fa-trophy circle-red"></i> WORD GAMES</div><div class="panel-body"><h4 class="setSize"><a class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-book circle-red"></i> Vocabulary Test</a></h4><h4 class="setSize"><a onclick="getQuizTitle(&quot;Grammar&quot;)" class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-font circle-red"></i> Grammar Test</a></h4><h4 class="setSize"><a class="spellChallenge" data-backdrop="static" data-toggle="modal" data-target="#spellChallenge" href="#"><i class="fa fa-pencil circle-red"></i> Spell Challenge</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#nameThing" href="#"><i class="fa fa-picture-o circle-red"></i> 4 Pics 1 Word</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#leaderBoardModal" href="#"><i class="fa fa-trophy circle-red"></i> Leader Boards</a></h4></div></div><div class="panel panel-primary side-posts blogPanel"><ul class="nav nav-tabs" role="tablist"><li role="presentation" class="active"><a href="#category" aria-controls="top" role="tab" data-toggle="tab">Category</a></li><li role="presentation"><a href="#recent" aria-controls="recent" role="tab" data-toggle="tab">Recent</a></li><li role="presentation"><a href="#top" aria-controls="top" role="tab" data-toggle="tab">Popular</a></li></ul><div class="panel-body sideBarPanelBody"><div class="tab-content"><div role="tabpanel" class="tab-pane active" id="category"><% for (var i = 0; i < param.categoryList.length; i++) { %><div class="media"><div class="row"><div class='col-xs-12'><div class="media-body"><a href="/blog/category/<%= param.categoryList[i].replace(/ /g, '-') %>"><h5 class="media-heading"><%= param.categoryList[i] %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="recent"><% for (var n = 0; n < param.blogList.length; n++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><img class="media-object" src="<%= param.blogList[n].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.blogList[n].title %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="top"><% for (var k = 0; k < param.topBlog.length; k++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><img class="media-object" src="<%= param.topBlog[k].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.topBlog[k].title %></h5></a></div></div></div></div><% } %></div></div></div></div></div></div></div>