<%- contentFor('body') %><div class="container"><div class="row"><div class="col-md-9"><div class="panel panel-primary wotd"><div class="panel-heading"><i class="fa fa-plus-square circle-blue"></i> <span id="newsFeedPageTitle">Add News Feed</span></div><div class="panel-body"><div class="container-fluid"><div class="success-msg hideMe" id="courseSuccessMsgID"></div><div class="errorMsg hideMe" id="courseErrorMsgID"></div><form id="addNewsFeedForm" class="form-horizontal marginTop10" enctype='multipart/form-data' method="post"><input type="hidden" id="newsFeedId"><div class="form-group"><select class="form-control" id="newsFeedDDL" required><option value="" disabled=disabled>Select News Feed Type</option><option value="quicktips">Quick Tips</option><option value="images">Images</option><option value="videos">Videos</option><option value="learningUsingRecentNews">Learn Using Recent News</option></select></div><div class="form-group"><select class="form-control" id="layoutTypeDDL" required><option value="" disabled=disabled>Select News Feed Layout</option><option value="imageSmall">Small Image</option><option value="imageLarge">Large Image</option><option value="video">Video</option><option value="text">Text</option></select></div><div class="form-group"><input type="text" class="form-control" placeholder="News Feed Title" name="title" id="newsFeedTitle" required></div><div class="form-group"><textarea class="form-control" id="input"></textarea></div><div class="form-group"><input type="text" class="form-control" placeholder="Video URL" name="title" id="newsFeedVideoUrl"></div><div class="form-group"><div class="row"><div class="col-md-6"><strong>Upload Image :</strong> <input type="file" id="newsFeedFileUpload"></div><div class="col-md-6" id="uploadEditImage"></div></div></div><div class="form-group special-buttons text-center"><button id="addNewsFeed-button" type="submit" class="btn btn-lg btn-login">Submit !</button></div></form></div></div><div class="panel-heading bottom"></div></div></div><div class="col-md-3 sidebar"><div class="panel panel-primary wog"><div class="panel-heading"><i class="fa fa-trophy circle-red"></i> WORD GAMES</div><div class="panel-body"><h4 class="setSize"><a class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-book circle-red"></i> Vocabulary Test</a></h4><h4 class="setSize"><a onclick="getQuizTitle(&quot;Grammar&quot;)" class=testVocab data-backdrop="static" data-toggle="modal" data-target="#testVocabModal" href="#"><i class="fa fa-font circle-red"></i> Grammar Test</a></h4><h4 class="setSize"><a class="spellChallenge" data-backdrop="static" data-toggle="modal" data-target="#spellChallenge" href="#"><i class="fa fa-pencil circle-red"></i> Spell Challenge</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#nameThing" href="#"><i class="fa fa-picture-o circle-red"></i> 4 Pics 1 Word</a></h4><h4 class="setSize"><a class="nameThing" data-backdrop="static" data-toggle="modal" data-target="#leaderBoardModal" href="#"><i class="fa fa-trophy circle-red"></i> Leader Boards</a></h4></div></div><div class="panel panel-primary side-posts blogPanel"><ul class="nav nav-tabs" role="tablist"><li role="presentation" class="active"><a href="#category" aria-controls="top" role="tab" data-toggle="tab">Category</a></li><li role="presentation"><a href="#recent" aria-controls="recent" role="tab" data-toggle="tab">Recent</a></li><li role="presentation"><a href="#top" aria-controls="top" role="tab" data-toggle="tab">Popular</a></li></ul><div class="panel-body sideBarPanelBody"><div class="tab-content"><div role="tabpanel" class="tab-pane active" id="category"><% for (var i = 0; i < param.categoryList.length; i++) { %><div class="media"><div class="row"><div class='col-xs-12'><div class="media-body"><a href="/blog/category/<%= param.categoryList[i].replace(/ /g, '-') %>"><h5 class="media-heading"><%= param.categoryList[i] %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="recent"><% for (var n = 0; n < param.blogList.length; n++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><img class="media-object" src="<%= param.blogList[n].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.blogList[n].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.blogList[n].title %></h5></a></div></div></div></div><% } %></div><div role="tabpanel" class="tab-pane" id="top"><% for (var k = 0; k < param.topBlog.length; k++) { %><div class="media"><div class="row"><div class='col-xs-3'><div class="media-left media-bottom"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><img class="media-object" src="<%= param.topBlog[k].urlthumb %>"></a></div></div><div class='col-xs-9'><div class="media-body"><a href="/blog/<%= param.topBlog[k].urlFriendlyTitle %>"><h5 class="media-heading"><%= param.topBlog[k].title %></h5></a></div></div></div></div><% } %></div></div></div></div></div></div></div>