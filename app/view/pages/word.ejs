<%- contentFor('body') %><div class="headerSection"><div class="container"><div class="Word"><a href="/">Home</a></div><div class="logo"><img src="/logo.svg"></div><div class="searchBar"><input id="searchWord" type="text" placeholder="Type a word or a phrase to search"> <button class="btn search-submit" type="button" onclick="search()"><span>Search</span></button></div><div class="searchBar center"><table align="center" class="wordCenterCss"><tr><th colspan="2"><h1><%= param.data.word %><% if(param.data.phoneticPronunciation) { %>[<%= param.data.phoneticPronunciation %>]<%}%></h1></th></tr><tr><% if(param.data.type) { %><td>Type:</td><td><%= param.data.type %></td><%}%></tr><tr><% if(param.data.englishMeaning) { %><td>English Meaning:</td><td><%= param.data.englishMeaning %></td><%}%></tr><tr><% if(param.data.hindiMeaning) { %><td>Hindi Meaning:</td><td><%= param.data.hindiMeaning %></td><%}%></tr><tr><% if(param.data.synonym) { %><td>Synonym :</td><td><%= param.data.synonym %></td><% } %></tr><tr><td colspan="2"><h4 class="definitionH4"><% if(param.data.definition) { %>Definitions :<% } %></h4><% if(param.data.definition) { %><ul class="definition-list"><% var definition = param.data.definition.split("|"); for(var i=0 ; i < definition.length ; i++){ %><li>&#8226;<%= definition[i] %></li><% } %></ul><% } %></td></tr><tr><td colspan="2"><% if(param.data.hindiSentence) { %><h4 class="usesSentance">Uses:</h4><% } %></td></tr><tr><% if(param.data.hindiSentence) { %><td>Hindi Sentence:</td><td><%= param.data.hindiSentence %></td><%}%></tr><tr><% if(param.data.englishSentence) { %><td>English Sentence:</td><td><%= param.data.englishSentence %></td><%}%></tr></table></div></div></div>